{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "es5",
    "sourceMap": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*"
  ]
}
